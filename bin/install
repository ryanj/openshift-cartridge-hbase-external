#!/bin/bash -e

source $OPENSHIFT_CARTRIDGE_SDK_BASH

client_result "hbase connection strings have been added to your application environment."
client_result ""
client_result "  HBASE_HOST=${HBASE_HOST}"
client_result "  HBASE_PORT=${HBASE_PORT}"
client_result "  HBASE_TABLE=${HBASE_TABLE}"
client_result ""

exit 0
