#!/bin/bash

source $OPENSHIFT_CARTRIDGE_SDK_BASH

function start() {
  echo "hbase connection strings available"
}

function stop() {
  echo "hbase connection strings available"
}

function restart() {
  echo "hbase connection strings available"
}

function status() {
  echo "hbase connection strings available"
}

function reload() {
  echo "hbase connection strings available"
}

function tidy() {
  echo "Nothing to clean"
}

case "$1" in
  start) start ;;
  stop) stop ;;
  restart) restart ;;
  status) status ;;
  reload) reload ;;
  tidy) tidy ;;
  *) exit 0
esac
